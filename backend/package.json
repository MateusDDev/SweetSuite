{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.x"
  },
  "main": "/build/server",
  "scripts": {
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:reset": "npx tsc && npm run db:drop && npm run db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:build": "npx tsc && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run db:reset",
    "start": "node ./build/server.js",
    "dev": "nodemon src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "test": "mocha -r ts-node/register tests/**/**/*.test.ts --exit -t 60000 --exit",
    "test:coverage": "nyc mocha -r ts-node/register tests/**/**/*.test.ts --exit -t 60000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-async-errors": "^3.1.1",
    "jest": "^29.7.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "9.1.0",
    "@types/node": "^20.11.30",
    "@types/sequelize": "^4.28.20",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.9",
    "@typescript-eslint/parser": "5.3.1",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "mocha": "9.2.1",
    "nodemon": "^3.0.2",
    "nyc": "15.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.0.4"
  }
}